   "version": "0.2.0",
   "configurations": [
      {
         "name": "helloworld - .NET Core Launch (console)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build helloworld",
         "program": "${workspaceFolder}/helloworld/bin/Debug/net6.0/helloworld.exe",
         "args": ["abc", "123"],
         "cwd": "${workspaceFolder}",
         "stopAtEntry": false,
         "console": "internalConsole"
      },
      {
         "name": "studentscores - .NET Core Launch (console)",
         "type": "coreclr",
         "request": "launch",
         "preLaunchTask": "build studentscores",
         "program": "${workspaceFolder}/studentscores/bin/Debug/net6.0/studentscores.exe",
         "args": ["studentscores/data/StudentScores.txt"],
         "cwd": "${workspaceFolder}",
         "stopAtEntry": false,
         "console": "internalConsole"
      }

   ]
}